<html lang="es">
<head>
    <title></title>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700&display=swap" rel="stylesheet">
    <style>
        strong {
            font-weight: 600;
        }
    </style>
</head>
<body style="font-family: 'Source Sans Pro', sans-serif; font-size: 16px; color: #fd0217; background-color: #f6f8f9; margin: 0; padding: 0">
<table width="650" align="center" cellpadding="0" cellspacing="0">
    <tr>
        <td style="height: 20px;"></td>
    </tr>
    <tr>
        <td style="border-bottom: 5px solid #fd0217; padding: 30px; text-align: center; background-color: #ffffff; color:#ffffff;">
            <a href="{{ app.request.getSchemeAndHttpHost() }}" style="color: #ffffff; text-decoration: none;">
                <img src="{{ email.image('@images/logo.svg') }}" alt=""/>
            </a>
        </td>
    </tr>
    <tr>
        <td style="padding: 30px 55px; background-color: #ffffff; text-align: center;">
            {% block main %}{% endblock %}
        </td>
    </tr>

    <tr>
        <td style="height: 20px; background-color: #ffffff; border-bottom: 5px solid #fd0217"></td>
    </tr>

    <tr>
        <td style="text-align: center; background-color: #ffffff; padding: 30px 60px;">
            <p>
                <a href="{{ app.request.getSchemeAndHttpHost() }}">
                    <img src="{{ email.image('@images/logo.svg') }}" alt="" style="height:25px;"/>
                </a>
            </p>
        </td>
    </tr>
    <tr>
        <td style="height: 20px;"></td>
    </tr>
</table>


